/* pages/details/index.wxss */
/*评价*/
.ajaxmore{display:block;width:100%;height:80rpx;line-height:80rpx;background:#efefef;color:#333;text-align:center}
.allevotenumber{height:80rpx;line-height:80rpx;width:720rpx;padding:0 15rpx;position:relative;display:flex;flex-wrap:wrap;border-bottom:2rpx solid #efefef;font-size:30rpx;margin-bottom:20rpx}
.allevoteBox{display:block;width:100%;height:70rpx;line-height:70rpx;background:#efefef;margin-top:30rpx;text-align:center;font-size:30rpx}
.allstart{margin-left:20rpx}
.allstart .score{color:#ff6700}
.evoteusername{height:60rpx;line-height:60rpx;padding:0 15rpx;width:720rpx;display:flex;flex-wrap:wrap;font-size:28rpx}
.evoteusername image{width:60rpx;height:60rpx;border:2rpx solid #efefef;box-sizing:border-box;margin-right:10rpx}
.userastar{width:250rpx;margin-left:20rpx;display:flex;flex-wrap:wrap}
.evoteitem{border-bottom:2rpx solid #efefef;padding-bottom:30rpx;padding-top:20rpx}
.istar{width:30rpx;height:30rpx;background:url(data:image/png;base64,)no-repeat center center;background-size:100% 100%;margin-top:15rpx;margin-right:10rpx}
.istar2{width:30rpx;height:30rpx;background:url(data:image/png;base64,)no-repeat center center;background-size:100% 100%;margin-top:15rpx;margin-right:10rpx}
.userVoteBox{padding:30rpx 30rpx 0;font-size:30rpx}
.container {
  margin: 0;
  padding: 0;
}
.userEvoteImg{overflow:hidden;width:720rpx;padding:30rpx 15rpx 0}
.userEvoteImg .vimg{width:120rpx;height:120rpx;overflow:hidden;float:left;margin-right:10rpx}