/* pages/details/index.wxss */

/*评价*/
@import "/pages/wxParse/wxParse.wxss";
.allevotenumber {
  height: 80rpx;
  line-height: 80rpx;
  width: 720rpx;
  padding: 0 15rpx;
  position: relative;
  display: flex;
  flex-wrap: wrap;
  border-bottom: 2rpx solid #efefef;
  font-size: 30rpx;
  margin-bottom: 20rpx;
}

.allevoteBox {
  display: block;
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  background: #efefef;
  margin-top: 30rpx;
  text-align: center;
  font-size: 30rpx;
}

.allstart {
  margin-left: 20rpx;
}

.allstart .score {
  color: #ff6700;
}

.evoteusername {
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 15rpx;
  width: 720rpx;
  display: flex;
  flex-wrap: wrap;
  font-size: 28rpx;
}

.evoteusername image {
  width: 60rpx;
  height: 60rpx;
  border: 2rpx solid #efefef;
  box-sizing: border-box;
  margin-right: 10rpx;
}

.userastar {
  width: 250rpx;
  margin-left: 20rpx;
  display: flex;
  flex-wrap: wrap;
}

.evoteitem {
  border-bottom: 2rpx solid #efefef;
  padding-bottom: 30rpx;
  padding-top: 20rpx;
}

.istar {
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,)no-repeat center center;
  background-size: 100% 100%;
  margin-top: 15rpx;
  margin-right: 10rpx;
}

.istar2 {
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,)no-repeat center center;
  background-size: 100% 100%;
  margin: 0 auto;
}

.userVoteBox {
  padding: 30rpx 30rpx 0;
  font-size: 30rpx;
}

.container {
  margin: 0;
  padding: 0;
}

.userEvoteImg {
  overflow: hidden;
  width: 720rpx;
  padding: 30rpx 15rpx 0;
}

.userEvoteImg .vimg {
  width: 120rpx;
  height: 120rpx;
  overflow: hidden;
  float: left;
  margin-right: 10rpx;
}

.swiper {
  width: 750rpx;
  height: 825rpx;
}

.count {
  color: #ff6700;
  font-size: 28rpx;
}

.details {
  padding-bottom: 100rpx;
}

.ctil {
  font-size: 32rpx;
  line-height: 2.5;
}

.protitle {
  padding: 10rpx;
  position: relative;
}

.startbox {
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  text-align: center;
}

.startbox text {
  font-size: 30rpx;
}

#content-tab image {
  width: 100%;
}

.protitle .bigtil {
  font-size: 40rpx;
  display: block;
}

.protitle .smalltitle {
  font-size: 30rpx;
  color: #666;
  line-height: 30rpx;
}

.protitle .price {
  color: #ff6700;
  font-size: 36rpx;
  display: block;
}

.protitle  .htit {
  font-size: 30rpx;
  color: #999;
  text-decoration: line-through;
  margin-left: 15rpx;
}

.chosebox {
  padding: 10rpx 5rpx;
  border-bottom: 2rpx solid #f5f5f5;
}

.chosebox .ctil {
  display: block;
  font-size: 32rpx;
  line-height: 36rpx;
  color: #666;
  padding-bottom: 6rpx;
}

.chose {
  padding-top: 12rpx;
  overflow: hidden;
}

.chose view {
  float: left;
  padding: 10rpx 20rpx;
  margin-right: 12rpx;
  font-size: 32rpx;
  background: #f5f5f5;
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
}

.chose .ck {
  background: #f37d6a;
  color: #fff;
}

.procontent {
  padding-top: 15rpx;
}

.protab {
  display: -webkit-flex;
  display: flex;
  position: relative;
  border-bottom: 10rpx solid #efefef;
}

.protab .swiper-tab-item {
  -webkit-box-flex: 1;
  flex: 1;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
}

.protab::before {
  display: block;
  content: '';
  width: 2rpx;
  height: 40rpx;
  background: #f5f5f5;
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}

.protab .active {
  color: #f37d6a;
}

.table .tr {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.table .td {
  height: 80rpx;
  line-height: 80rpx;
  border: 1rpx solid #f5f5f5;
  padding-left: 10rpx;
  font-size: 30rpx;
}

.table .left {
  width: 222rpx;
}

.table .right {
  width: 503rpx;
}

.detailsbuy {
  display: flex;
  display: -webkit-flex;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  background: #fff;
  width: 100%;
  z-index: 12;
}

.detailsbuy .item2, .detailsbuy .item3 {
  -webkit-flex: 4;
  flex: 2;
  text-align: center;
  line-height: 100rpx;
  font-size: 32rpx;
}

.detailsbuy .item {
  color:#666;
  font-size: 26rpx;
  position: relative;
  flex: 1;
}
.carImg{
  display: block;
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 17px auto;
}
.start {
  display: block;
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%;
}

.detailsbuy .carnum {
  display: block;
  width: 30rpx;
  height: 30rpx;
  text-align: center;
  line-height: 30rpx;
  background: #f00;
  color: #fff;
  font-size: 24rpx;
  -webkit-border-radius: 50%;
  border-radius: 50%;
  position: absolute;
  right: 12rpx;
  top: 1rpx;
}
/*加入购物车*/
.detailsbuy .item2 {
  background: #f37d6a ;
  color: #fff;
}
/*移出购物车*/
.detailsbuy .item3 {
  background: #f5f5f5;
}
/*加入收藏*/
.detailsbuy .item4 {
  position: relative;
  flex: 1;
  color:#666;
  font-size: 26rpx;
  
}
/*转发*/
.detailsbuy .item5 {
  position: relative;
  flex: 1;
  color:#666;
  font-size: 26rpx;
  background:transparent;
  display:flex;
  justify-content:center;
  margin-right:-30rpx;
  margin-left:-30rpx;

}
button::after {
  border: none;
}
.verbImg{
  display: block;
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%;
}
.numbox {
  padding-top: 10rpx;
  overflow: hidden;
  padding-left: 15rpx;
  padding-right: 15rpx;
}

.numbox text {
  float: left;
  line-height: 60rpx;
  font-size: 30rpx;
}

.numbox .xm-input-number {
  border: 1rpx solid #eee;
  width: 227rpx;
  float: right;
}

.numbox .input-sub, .numbox .input-add {
  height: 60rpx;
  line-height: 60rpx;
  float: left;
  width: 60rpx;
  text-align: center;
  background: #fafafa;
}

.numbox .inout-num {
  width: 107rpx;
  float: left;
  height: 60rpx;
  text-align: center;
  font-size: 30rpx;
}

.swiper-box {
  display: block;
  height: 100%;
}
