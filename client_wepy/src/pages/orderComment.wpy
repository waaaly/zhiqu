<script>

import wepy from 'wepy';
import http from '../utils/Base';
import api from '../utils/API'; 
const uploadFile = require("../utils/Upload.js");
export default class orderComment extends wepy.page{

  data = {
    current:0,
    attitude:true,
    time: true,
    efficiency: true,
    environment:true,
    professional:true,
    userStars: [
      "star-on",
      "star-on",
      "star-on",
      "star-on",
      "star-on",
    ],
    wjxScore:5,
    textarea:"",//评论内容
    currentWordNumber:0,//当前字数
    min: 5,//最少字数
    max: 300, //最多字数 (根据自己需求改变) 
    pics:[],
    pisUrl:[],//oss图片存储地址
    shopId:"",//当前评价的订单属于哪个商家
    addPic:true,
  }
  onLoad(e){
    // console.log(e)
    this.shopId = e.shop_id;
    this.orderId = e.order_id;
    this.pics = [];
    this.picUrl = [];
  }
  onShow(e){
    
  }
  onReady(e){
    this.drawProgressbg(); 
    
  }
  methods = {
      // 星星点击事件
        starTap: function (e) {
            var index = e.currentTarget.dataset.index; // 获取当前点击的是第几颗星星

            switch(index){
              case 0:{
                this.userStars = ["star-on",
                                  "star-off",
                                  "star-off",
                                  "star-off",
                                  "star-off",];
                break;
              }
              case 1:{
                this.userStars = ["star-on",
                                  "star-on",
                                  "star-off",
                                  "star-off",
                                  "star-off",];
                break;
              }
              case 2:{
                this.userStars = ["star-on",
                                  "star-on",
                                  "star-on",
                                  "star-off",
                                  "star-off",];
                break;
              }
              case 3:{
                this.userStars = ["star-on",
                                  "star-on",
                                  "star-on",
                                  "star-on",
                                  "star-off",];
                break;
              }
              case 4:{
                this.userStars = ["star-on",
                                  "star-on",
                                  "star-on",
                                  "star-on",
                                  "star-on",];
                break;
              }
            }
            this.wjxScore = index + 1;
            this.$apply();
        },
        // 留言
        //字数限制  
        inputs: function (e) {
            // 获取输入框的内容
            var value = e.detail.value;
            this.textarea = value;
            // 获取输入框内容的长度
            var len = parseInt(value.length);
            //最多字数限制
            if (len > this.data.max) return;
            // 当输入框内容的长度大于最大长度限制（max)时，终止setData()的执行          
            this.currentWordNumber=len //当前字数    
            this.drawCircle(len/300*2);         
        },
        // 图片
        choose: function (e) {//这里是选取图片的方法
            var that = this;
            var userId = wx.getStorageSync('userInfoInServer').id
            var perfix = 'front/' + userId +'/';

            wx.chooseImage({
              count: 3 - that.pics.length, // 最多可以选择的图片张数，默认9
              sizeType: ['original', 'compressed'], // original 原图，compressed 压缩图，默认二者都有
              sourceType: ['album', 'camera'], // album 从相册选图，camera 使用相机，默认二者都有
              success: function (res) {

                 var imgsrc = res.tempFilePaths;
                 var fileName;
                 for (let item of imgsrc){
                   
                    if(item.indexOf('http://tmp')!=-1){
                      fileName = item.replace('http://tmp/', perfix)
                    }else{
                      fileName = item.replace('wxfile://tmp_', perfix)
                    }
                    console.log(fileName);
                    that.picUrl.push(fileName);    
                    uploadFile(item,userId);
                 }
                  that.pics = that.pics.concat(imgsrc);//连接两个或多个数组 
                  if(that.pics.length >= 3){
                    that.addPic = false;
                  }          
                  that.$apply();       
              }
            })
        },
        //这里触发图片上传的方法
        comment: function () {
          var userId = wx.getStorageSync('userInfoInServer').id;
          http.post(api.UserComment,{shop_id: this.shopId,
                    user_id: userId,
                    order_id: this.orderId,
                    content: this.textarea,
                    grade: this.wjxScore,
                    list_pic_url: this.picUrl,
          }).then(res=>{
            if(res.msg =="评论成功"){
              wx.showToast({
                title:'评价成功',
              })
              wx.navigateBack({

              })
            }
          })
        },
        // 删除图片
        deleteImg: function (e) {
            var pics = this.data.pics;
            var index = e.currentTarget.dataset.index;
            this.pics.splice(index, 1);
            this.$apply();
        },
        // 预览图片
        previewImg: function (e) {
            //获取当前图片的下标
            var index = e.currentTarget.dataset.index;
            //所有图片
            var pics = this.data.pics;
            wx.previewImage({
            //当前显示图片
            current: pics[index],
            //所有图片
            urls: pics
            })
        },
  }
  drawProgressbg(){
    // 使用 wx.createContext 获取绘图上下文 context
    var ctx = wx.createCanvasContext('canvasProgressbg',this)
    ctx.setLineWidth(4);// 设置圆环的宽度
    ctx.setStrokeStyle('#20183b'); // 设置圆环的颜色
    ctx.setLineCap('round') // 设置圆环端点的形状
    ctx.beginPath();//开始一个新的路径
    ctx.arc(110, 110, 100, 0, 2 * Math.PI, false);
    //设置一个原点(110,110)，半径为100的圆的路径到当前路径
    ctx.stroke();//对当前路径进行描边
    ctx.draw();
  }
  drawCircle (step){  
    var context = wx.createCanvasContext('canvasProgress',this);
      // 设置渐变
      var gradient = context.createLinearGradient(200, 100, 100, 200);
      gradient.addColorStop("0", "#2661DD");
      gradient.addColorStop("0.5", "#40ED94");
      gradient.addColorStop("1.0", "#5956CC");
      
      context.setLineWidth(10);
      context.setStrokeStyle(gradient);
      context.setLineCap('round')
      context.beginPath(); 
      // 参数step 为绘制的圆环周长，从0到2为一周 。 -Math.PI / 2 将起始角设在12点钟位置 ，结束角 通过改变 step 的值确定
      context.arc(110, 110, 100, -Math.PI / 2, step * Math.PI - Math.PI / 2, false);
      context.stroke(); 
      context.draw() 
  }
}
</script>

