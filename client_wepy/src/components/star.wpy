<template>
<view class=" star {{starType}}">
    <label wx:for="{{itemClasses}}" wx:for-index="index"  class=" star-item {{itemClass}}" wx:key="itemClass.index" key="{{itemClass.index}}" wx:for-item="itemClass"></label>
  </view>
</template>

<script>
const LENGTH = 5;
const CLS_ON = 'on';
const CLS_HALF = 'half';
const CLS_OFF = 'off';

import wepy from "wepy";

export default class star extends wepy.component{
    props = {
      grade:{
        type:String,
        default:5
      }
    }
    data = {
        size:24,
        starType:"",
        itemClasses:[],
    }
    onLoad(e){

       this.starType = 'star-' + this.size;
    }
    watch = {
      grade(_new,old){
        console.log(_new)
      }
    }
    //接受伏组件发送的广播事件，相当于初始化
    events = {
        'initStar': (size,score, $event) => {
            this.size = size;
            this.starType = 'star-' + this.size;
            console.log(score);
            let result = [];
            let _score = Math.floor(score * 2) / 2;
            let hasDecimal = _score % 1 !== 0;
            let integer = Math.floor(_score);
            for (let i = 0; i < integer; i++) {
                result.push(CLS_ON)
            }
            if (hasDecimal) {
                result.push(CLS_HALF)
            }
            while (result.length < LENGTH) {
                result.push(CLS_OFF)
            }
            console.log(result);
            this.itemClasses = result;
            this.$apply();
            }
    }

}
</script>

