<template>

<view class=" goods">
      <scroll-view style="width:100px" scroll-y class="  menu-wrapper" scroll-into-view="{{navId}}" scroll-with-animation="true">
        <view class="_ul  ">
          <view wx:for="{{goods}}" wx:key="index" key="{{index}}" class="menu-item {{index === currentIndex ? 'current' : ''}}" bindtap="selectMenu" id="{{'nav_'+index}}" data-eventid="{{index}}"   wx:for-index="index"
            wx:for-item="item">
            <label class="text">
              <label wx:if="{{item.type>0}}" class="  icon {{classMap[item.type]}}"></label>{{item.name}}
            </label>
          </view>
        </view>
      </scroll-view>
      <scroll-view style="height:100%" scroll-y scroll-into-view="{{toviewid}}" scroll-with-animation="true" class="  foods-wrapper" bindscroll="scroll" data-eventid="{{'2'}}" >
        <view class="_ul ">
          <view wx:for="{{goods}}" id="{{'con_'+i}}" wx:key="i" key="{{i}}" class="_li  food-list food-list-hook"  wx:for-index="i" wx:for-item="item">
            <view class="_h1  title">{{item.name}}</view>
            <view class="_ul ">
              <view bindtap="selectFood" wx:key="subIndex" key="{{subIndex}}" class="_li  food-item border-1px" data-eventid="{{'1-'+i+'-'+subIndex}}"  wx:for="{{item.foods}}" wx:for-index="subIndex" wx:for-item="food">
                <view class=" icon">
                  <image src="{{food.icon}}" class="_img "></image>
                </view>
                <view class=" content">
                  <view class="_h2  name">{{food.name}}</view>
                  <view class="_p  desc">{{food.description}}</view>
                  <view class=" extra">
                    <label class="  count">月售{{food.sellCount}}份</label>
                    <label class=" ">好评率{{food.rating}}%</label>
                  </view>
                  <view class=" price">
                    <label class="  now">¥{{food.price}}</label>
                    <label class="  old" hidden="{{!(food.oldPrice)}}">{{food.oldPrice}}</label>
                  </view>
                  <view class=" cartcontrol-wrapper">
                    <view class="cartcontrol">
                        <transition name="move" >
                        <view class="cart-decrease"  catchtap="decreaseCount" data-eventid="{{'0'}}" data-comkey="{{$k}}">
                            <label class="inner iconfont"></label>
                        </view>
                        </transition>
                        <view class="cart-count" wx:if="{{food.count>0}}">{{food.count}}</view>
                        <view class="cart-add iconfont" catchtap="addCart" data-eventid="{{'1'}}" data-comkey="{{$k}}"></view>
                    </view>
                  </view>
                </view>
              </view>
            </view>
          </view>
        </view>
      </scroll-view>
    </view>
</template>
<script>
import wepy from "wepy";

export default class shopGood extends wepy.component{
    props = {
        food:{},
    }
    data = {
        goods:[],
        currentIndex:0,
        navId: '',
        toviewid: '',
        classMap:['decrease', 'discount', 'special', 'invoice', 'guarantee'],
    }
    onLoad(e){
    wx.request({
      url: 'https://www.easy-mock.com/mock/5c0515eef57279499b6ac104/getgoods',
      methods: 'GET',
      success: (res) => {
        this.goods = res.data.goods;
        this.$apply();
        console.log(res);
      }
    })
    }

    methods = {
        selectMenu(e){
            let index = e.currentTarget.dataset.eventid
            // console.log(e);
            this.currentIndex = index;
            this.toviewid = `con_${index}`;
            this.navId = `nav_${index}`;
            this.$apply();
            console.log(this.toviewid + this.navId)
        },
        scroll(e){
            // this.toviewid = '';
            // let scrollTop = e.target.scrollTop;
            // let length = this.listHeight.length;
            // if (scrollTop > 0 && scrollTop < this.listHeight[0]) {
            //     this.currentIndex = 0
            // }
            // for (let i = 0; i < length; i++) {
            //     if (scrollTop >= this.listHeight[i - 1] && scrollTop < this.listHeight[i]) {
            //     this.currentIndex = i
            //     }
            // }
        },
        addCart (event) {
        // console.log('click')
            if (!this.food.count) {
                Vue.set(this.food, 'count', 1)
            } else {
                this.food.count++
            }
            this.$emit('cart-add', event.target)
        },
        decreaseCount () {
            if (this.food.count) {
                this.food.count--
                }
        }
    }
}
</script>

